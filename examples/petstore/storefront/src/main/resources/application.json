{
  "micronaut": {
    "server": {
      "port": 8080,
      "cors": {
        "enabled": true,
        "configurations": {
          "web": {
            "allowedMethods": ["POST", "GET"]
          }
        }

      }
    },
    "application": {
      "name":"storefront"
    }
  },
  "consul": {
    "client": {
      "registration":{
          "enabled":false
      },
      "defaultZone":"${CONSUL_HOST:localhost}:${CONSUL_PORT:8500}"
    }
  },
  "router": {
    "static": {
      "resources": {
        "enabled": true,
        "mapping": "/**",
        "paths": ["classpath:static"]
      }
    }
  }
}


