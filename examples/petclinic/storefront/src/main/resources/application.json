{
  "particle": {
    "server": {
      "port":"8080",
      "cors": {
        "enabled": true
      }
    },
    "application": {
      "name":"storefront"
    }
  },
  "consul": {
    "client": {
      "registration":{
          "enabled":false
      },
      "defaultZone":"${CONSUL_HOST:localhost}:${CONSUL_PORT:8500}"
    }
  },
  "router": {
    "static": {
      "resources": {
        "enabled": true,
        "mapping": "/assets/**",
        "paths": ["classpath:static"]
      }
    }
  }
}


